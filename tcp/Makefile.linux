
CC = gcc
AR = ar
RANLIB = ranlib
CFLAGS = -I. -I../ip-linux/ -g -Wall -DDEBUG -ansi
#CFLAGS = -I. -I../ip-linux/ -g -Wall -DDEBUG

all: libtcp.a

libtcp.a: tcp.o util.o
	$(AR) rv libtcp.a tcp.o util.o
	$(RANLIB) libtcp.a

tcp.o: tcp.c tcp.h
	$(CC) $(CFLAGS) -c -o $@ $<

util.o: util.c
	$(CC) $(CFLAGS) -c -o $@ $<

clean:
	-rm *.o
	-rm *.a

